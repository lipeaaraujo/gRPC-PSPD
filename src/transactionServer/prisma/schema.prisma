generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://user:12345@localhost:5433/db_grpc_transaction"
}

model Transaction {
    id String @id
    client_id String 
    value Float
    type String
    description String
    date DateTime
}
